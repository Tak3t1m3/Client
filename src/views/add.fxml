<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXToggleButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="344.0" prefWidth="185.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ControllerFarmer">
   <children>
      <JFXButton alignment="CENTER" buttonType="RAISED" layoutX="59.0" layoutY="300.0" onAction="#addCrop" text="Add Crop">
         <font>
            <Font name="Microsoft JhengHei Bold" size="13.0" />
         </font>
      </JFXButton>
      <TextField fx:id="cName" alignment="CENTER" layoutX="7.0" layoutY="83.0" promptText="Name" />
      <TextField fx:id="cWeight" alignment="CENTER" layoutX="7.0" layoutY="118.0" promptText="Weight" />
      <TextField fx:id="cCost" alignment="CENTER" layoutX="8.0" layoutY="153.0" promptText="Cost" />
      <TextField fx:id="cQuantity" alignment="CENTER" layoutX="8.0" layoutY="188.0" promptText="quantity" />
      <JFXToggleButton fx:id="cAvail" layoutX="30.0" layoutY="215.0" text="Available" toggleColor="#ffcb00" toggleLineColor="#c69b00">
         <font>
            <Font name="Microsoft JhengHei Bold" size="13.0" />
         </font>
      </JFXToggleButton>
      <ImageView fx:id="cropImage" fitHeight="81.0" fitWidth="136.0" layoutX="13.0" pickOnBounds="true" preserveRatio="true" />
      <JFXButton layoutX="27.0" layoutY="8.0" onAction="#cropImage" prefHeight="67.0" prefWidth="128.0" style="-fx-border-color: #ffcb00; -fx-cursor: hand;" text="Image" textFill="#ffcb00">
         <font>
            <Font name="Microsoft JhengHei Bold" size="12.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
